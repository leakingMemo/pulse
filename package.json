{
  "name": "pulse",
  "title": "Pulse",
  "description": "Your life's pulse, one keystroke away",
  "icon": "pulse-icon.png",
  "author": "nathandsouza",
  "license": "MIT",
  "commands": [
    {
      "name": "dashboard",
      "title": "Open Dashboard",
      "subtitle": "View your life metrics",
      "description": "Quick access to finance, health, tasks, and habits",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.81.2",
    "@raycast/utils": "^1.16.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^8.57.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "clean": "rm -rf dist"
  }
}
