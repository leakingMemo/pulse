{
  "name": "pulse",
  "title": "Pulse",
  "description": "Your life's pulse, one keystroke away",
  "icon": "pulse-icon.png",
  "author": "nathandsouza",
  "license": "MIT",
  "commands": [
    {
      "name": "dashboard",
      "title": "Open Dashboard",
      "subtitle": "View your life metrics",
      "description": "Quick access to finance, health, tasks, and habits",
      "mode": "view",
      "preferences": [
        {
          "name": "hotkey",
          "type": "textfield",
          "required": false,
          "title": "Hotkey",
          "description": "Keyboard shortcut (e.g., F4)",
          "default": "F4"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.81.2",
    "@raycast/utils": "^1.16.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "^18.2.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
